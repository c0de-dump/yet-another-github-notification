{
    "name": "yane-github",
    "version": "1.0.0",
    "description": "Yet Another Notification Extension for Github",
    "scripts": {
        "start": "web-ext -f=/Applications/Firefox.app/Contents/MacOS/firefox run",
        "prestart:dev": "npm run build:dev",
        "start:dev": "web-ext run -f=/Applications/Firefox.app/Contents/MacOS/firefox --verbose --devtools",
        "build:dev": "webpack --mode=development",
        "build:prod": "webpack --mode=production --node-env=production"
    },
    "keywords": [
        "Github",
        "Notification",
        "Extension"
    ],
    "author": "",
    "license": "MIT",
    "devDependencies": {
        "@babel/core": "^7.23.3",
        "@babel/preset-env": "^7.23.3",
        "@babel/preset-typescript": "^7.23.3",
        "@types/webextension-polyfill": "^0.10.6",
        "babel-loader": "^9.1.3",
        "copy-webpack-plugin": "^11.0.0",
        "prettier": "^3.0.1",
        "typescript": "^5.2.2",
        "web-ext": "^7.6.2",
        "webextension-polyfill": "^0.10.0",
        "webpack": "^5.89.0",
        "webpack-cli": "^5.1.4"
    },
    "volta": {
        "node": "16.20.2"
    }
}
